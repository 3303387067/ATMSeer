{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "jsRelus":{
    "object-literal-sort-keys": false,
    "no-unused-variable": false,
    "ordered-imports": false
  },
  "relus":{
    "no-unused-variable": false,
    "ordered-imports": false,
    "no-console": [true, "log", "error"]
  }
}
